{
  "Module Header": {
    "prefix": "module",
    "body": [
      "module $1",
      "in $2",
      "out $3",
      "use ${4:Std}",
      "",
      "$0"
    ]
  },
  "Data Definition": {
    "prefix": "data",
    "body": "data ${1:id} = ${2:exp}"
  },
  "If Expression": {
    "prefix": "if",
    "body": "if $1 then $2 else $3"
  }
}
